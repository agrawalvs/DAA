    "def get_freq(msg):\n",
    "    msglist = []\n",
    "    for i in msg:\n",
    "        msglist.append(i)\n",
    "    msgset = set(msglist)\n",
    "    freq =[]\n",
    "    for i in msgset:\n",
    "        freq.append((i,msglist.count(i)))\n",
    "    freq.sort(key = lambda x: x[1])\n",
    "    return freq\n",
